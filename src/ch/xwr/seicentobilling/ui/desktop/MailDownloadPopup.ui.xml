<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?xdevuiml version="1.0"?>
<?template BROWSER_XGA?>
<?import com.xdev.ui.XdevButton?>
<?import com.xdev.ui.XdevGridLayout?>
<?import com.xdev.ui.XdevLabel?>
<?import com.xdev.ui.XdevView?>
<XdevView xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="MailDownloadPopup">
    <x:constraints zpos="0" width="100%" height="100%" />
    <XdevGridLayout x:name="gridLayout">
        <x:constraints zpos="0" width="100%" height="100%" />
        <XdevButton caption="Erstelle Datei" x:name="cmdStart">
            <x:constraints gridx="1" gridy="1" weighty="0.0" weightx="0.0" width="0px" height="0px" />
            <x:event>click.buttonClick</x:event>
        </XdevButton>
        <XdevButton icon="WebContent/WEB-INF/resources/images/Download1.png" caption="Download" enabled="false" x:name="cmdDownload">
            <x:constraints gridx="2" gridy="1" weighty="0.0" weightx="0.0" width="0px" height="0px" />
            <x:event>click.buttonClick</x:event>
        </XdevButton>
        <XdevButton icon="WebContent/WEB-INF/resources/images/mail1.png" caption="Ã–ffne Mail" enabled="false" x:name="cmdMail">
            <x:constraints gridx="3" gridy="1" weighty="0.0" weightx="0.0" width="0px" height="0px" />
            <x:event>click.buttonClick</x:event>
        </XdevButton>
        <XdevLabel x:name="label" value="Erstelle Datei...">
            <x:constraints gridx="1" gridy="2" weighty="0.0" weightx="0.0" width="100%" gridWidth="3" height="0px" />
        </XdevLabel>
    </XdevGridLayout>
</XdevView>

